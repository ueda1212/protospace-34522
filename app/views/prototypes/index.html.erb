<main class="main">
  <div class="inner">
  <%= render partial: 'prototype', collection: @prototypes %>
    <%# ログインしているときは以下を表示する %>
    <% if user_signed_in? %>
      <div class="greeting">
      <div>こんにちは</div>
        <%= link_to current_user.name, user_path(current_user), class: :greeting__link%>
        <div>さん</div>
       </div>
    <%# // ログインしているときは上記を表示する %>
    <div class="card__wrapper">
      <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
       <%= render partial: 'prototype', collection: @prototypes %>
    </div>
    <% end %>
  </div>
</main>
